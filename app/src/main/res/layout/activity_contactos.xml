        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton4"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:srcCompat="@mipmap/Reloj" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="112dp"
        android:layout_marginLeft="112dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="67dp"
        android:layout_marginRight="67dp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton5"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:srcCompat="@mipmap/Ubicacion" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="442.000.0000"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="220dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="134dp"
        android:layout_height="47dp"
        android:text="Circuito Puerta del Sol, Angel Aniel 701"
        tools:layout_editor_absoluteX="251dp"
        tools:layout_editor_absoluteY="310dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10:00 am to 22:00 pm"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="220dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="151dp"
        android:layout_height="54dp"
        android:text="Contacto@Astros.com  ventas_astros@Astros.com"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="310dp" />

    <EditText
        android:id="@+id/editTextTextPersonName9"
        android:layout_width="306dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="480dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

</androidx.constraintlayout.widget.ConstraintLayout>